{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-activitylog": "^2.5",
        "dingo/api": "2.0.0-alpha1",
        "tymon/jwt-auth": "1.*",
        "roomify/bat": "^1.2",
        "watson/validating": "^3.1",
        "itsgoingd/clockwork": "^2.2",
        "barryvdh/laravel-cors": "^0.11.0",
        "shipping-docker/vessel": "^3.0",
        "predis/predis": "^1.1",
        "maatwebsite/excel": "^3.0",
        "league/csv": "^9.1",
        "skagarwal/google-places-api": "^1.2",
        "cviebrock/eloquent-sluggable": "^4.4",
        "parsecsv/php-parsecsv": "^1.0",
        "phpoffice/phpspreadsheet": "^1.2",
        "spatie/regex": "^1.2",
        "illuminate/mail": "5.5.35"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
